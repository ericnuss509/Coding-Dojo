<a asp-action="Dashboard">Dashboard</a> |
<a asp-action="Logout" asp-controller="User">Log Out</a>

@model Wedding
<form asp-action="CreateWedding" method="post">
    <div class="form-group">
    <label asp-for="Bride"></label>
    <input asp-for="Bride" class="form-control">
    <span asp-validation-for="Bride" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Groom"></label>
        <input asp-for="Groom" class="form-control">
        <span asp-validation-for="Groom" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Date"></label>
        <input asp-for="Date" class="form-control">
        <span asp-validation-for="Date" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Location"></label>
        <input asp-for="Location" class="form-control">
        <span asp-validation-for="Location" class="text-danger"></span>
    </div>
    <button>Submit</button>
</form>